#include <iostream>

int main() {
    int num1, num2, num3;

    std::cout << "Enter three integers separated by spaces: ";
    std::cin >> num1 >> num2 >> num3;

    int maxNum = num1; 

    if (num2 > maxNum) {
        maxNum = num2; 
    }

    if (num3 > maxNum) {
        maxNum = num3; 
    }

    std::cout << "The greatest integer among " << num1 << ", " << num2 << ", and " << num3 << " is: " << maxNum << std::endl;

    return 0;
}
